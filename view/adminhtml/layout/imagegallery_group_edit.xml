      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="editor"/>
    <body>
        <referenceContainer name="content">
            <block class="Magenest\ImageGallery\Block\Adminhtml\Group\Edit" name="imagegallery_group_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Magenest\ImageGallery\Block\Adminhtml\Group\Edit\Tabs" name="imagegallery_group_edit_tabs">
            </block>
        </referenceContainer>

        <referenceContainer name="js">
            <block class="Magenest\ImageGallery\Block\Adminhtml\Group\Edit\Js" template="jsgroup.phtml"/>
        </referenceContainer>
    </body>
</page>
